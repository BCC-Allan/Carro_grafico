   "source": [
    "def bezier(p1: tuple, p2: tuple, p3: tuple, t: float): \n",
    "    x1 = p1[0]\n",
    "    x2 = p2[0]\n",
    "    x3 = p3[0]\n",
    "    y1 = p1[1]\n",
    "    y2 = p2[1]\n",
    "    y3 = p3[1]\n",
    "    x = (1-t)**2 * x1 + 2 * (1-t)*t * x2 + t**2 * x3\n",
    "    y = (1-t)**2 * y1 + 2 * (1-t)*t * y2 + t**2 * y3\n",
    "    return (x, y)"
   ]
  },
